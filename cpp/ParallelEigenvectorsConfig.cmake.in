# - Config file for ParallelEigenvectors
# It defines the following variables
#  PARALLELEIGENVECTORS_FOUND        - the library has been found
#  PARALLELEIGENVECTORS_INCLUDE_DIRS - include directories for ParallelEigenvectors
#  PARALLELEIGENVECTORS_LIBRARIES    - libraries to link against

get_filename_component(PARALLELEIGENVECTORS_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(PARALLELEIGENVECTORS_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

if(NOT TARGET parallel_eigenvectors AND NOT ParallelEigenvectors_BINARY_DIR)
	include("${PARALLELEIGENVECTORS_CMAKE_DIR}/ParallelEigenvectorsTargets.cmake")
endif()

set(PARALLELEIGENVECTORS_LIBRARIES)
set(PARALLELEIGENVECTORS_FOUND TRUE)
